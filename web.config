  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <!--Clickjacking security, XSS-Protection, HSTS(Header Strict-Transport-Security)-->
      <httpProtocol>
        <customHeaders>
          <add name="X-Frame-Options" value="DENY" />
          <add name="X-XSS-Protection" value="1; mode=block" />
          <add name="X-Content-Type-Options" value="nosniff" />
          <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains" />
          <remove name="X-Powered-By" />
        </customHeaders>
      </httpProtocol>
      <!--End clickjacking , XSS-Protection, HSTS(Header Strict-Transport-Security)-->
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath=".\OwnYITCSAT.exe" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
    </system.webServer>
    <!--Cookie HTTPOnly Flag-->
    <system.web>
      <httpCookies httpOnlyCookies="true" requireSSL="true" />
    </system.web>
    <!--End Cookie HTTPOnly Flag-->
  </location>
