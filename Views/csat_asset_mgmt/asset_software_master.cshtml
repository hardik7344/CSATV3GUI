<script type="text/javascript">

    function Check1() {
        if (document.getElementById('subscription').checked) {
            document.getElementById('subscription_user').style.display = 'block';
            document.getElementById('perpetual_user').style.display = 'none';
        }

        if (document.getElementById('perpetual').checked) {
            document.getElementById('perpetual_user').style.display = 'block';
            document.getElementById('subscription_user').style.display = 'none';

        }
        if (document.getElementById('subscriptionreport').checked) {
            document.getElementById('subscription_report').style.display = 'block';
            document.getElementById('perpetual_report').style.display = 'none';
        }

        if (document.getElementById('perpetualreport').checked) {
            document.getElementById('perpetual_report').style.display = 'block';
            document.getElementById('subscription_report').style.display = 'none';

        }

    }
    function Check2() {
        if (document.getElementById('editsubscription').checked) {
            document.getElementById('subscription_user1').style.display = 'block';
            document.getElementById('perpetual_user1').style.display = 'none';
        }

        if (document.getElementById('editperpetual').checked) {
            document.getElementById('perpetual_user1').style.display = 'block';
            document.getElementById('subscription_user1').style.display = 'none';

        }


    }

</script>
