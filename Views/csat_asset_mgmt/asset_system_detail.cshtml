    // Add Asset Data
    function AddAssetdata() {
        deviceid=@ViewBag.deviceid;
        var url = '@Url.Content("~/csat_asset_mgmt/Gethrdjsondata?deviceid=")' + deviceid;
        $.ajax({
            type: "POST",
            url: url,
            success: function (data) {
            // debugger;
                ramsize = data.ramSize;
                hddsize = data.hddSize;
                for (var i = 0; i < data.hardwaretype.length; i++) {
                    if ('BaseBoard' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.baseboard.length; j++) {
                            var Motherboarddata = JSON.parse(data.baseboard[j].hw_json);
                            Motherboard = Motherboarddata.Name;
                            Manufacturer = Motherboarddata.Manufacturer;
                        }
                    }
                    if ('Processor' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.proc.length; j++) {
                            var Processordata = JSON.parse(data.proc[j].hw_json);
                            Processor = Processordata.Name;
                        }
                    }
                    if ('ComputerSystem' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.comp.length; j++) {
                            var Modeldata = JSON.parse(data.comp[j].hw_json);
                            Model = Modeldata.Model;
                        }
                    }

                    if ('PhysicalMemory' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.pm.length; j++) {
                            var ramdata = JSON.parse(data.pm[j].hw_json);
                            if (ramdata.MemoryType == '20')
                                ramdata.MemoryType = 'DDR';
                            else if (ramdata.MemoryType == '21')
                                ramdata.MemoryType = 'DDR2';
                            else if (ramdata.MemoryType == '17')
                                ramdata.MemoryType = 'SDRAM';
                            else if (ramdata.MemoryType == '24')
                                ramdata.MemoryType = 'DDR3';
                            else
                                ramdata.MemoryType = 'Unknown';
                            ramtype += ',' + ramdata.MemoryType;
                        }
                    }
                    if ('Keyboard' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.key.length; j++) {
                            var KeyBoarddata = JSON.parse(data.key[j].hw_json);
                            keyboard = KeyBoarddata.Name;
                        }
                    }
                    if ('PointingDevice' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.pd.length; j++) {
                            var mousedata = JSON.parse(data.pd[j].hw_json);
                            mouse = mousedata.Name;
                        }
                    }
                    if ('DesktopMonitor' == data.hardwaretype[i].hardware_class) {
                        for (j = 0; j < data.monitor.length; j++) {
                            var monitordata = JSON.parse(data.monitor[j].hw_json);
                            monitor = monitordata.Name;
                        }
                    }
                }
                var url1 = '@Url.Content("~/csat_asset_mgmt/store_assetinfo?deviceid=")' + deviceid + '&motherboard=' + Motherboard + '&manufacturer=' + Manufacturer + '&processor=' + Processor + '&model=' + Model + '&hddcapacity=' + hddsize + '&ramtype=' + ramtype + '&ramsize=' + ramsize + '&keyboard=' + keyboard + '&mouse=' + mouse + '&monitor=' + monitor + '&os=' + data.os + '&osdate=' + data.date;
                $.ajax({
                    type: "POST",
                    url: url1,
                    success: function (data) {

                    }
                });
            }
        });

    }


