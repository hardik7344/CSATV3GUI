@{
    ViewData["Title"] = "Session Timeout";
    Layout = "~/Views/csat_login/login_main_page.cshtml";
}


<div class="row ">
    <div class="col-lg-4 col-md-3 col-sm-3 col-xs-2"></div>
    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-8 shadow" style="margin-top: 150px;background-color: #fff;">
        <form action="#">
            <div class="form-group" style="margin-top:15px;">
                <label class="col-sm-2 col-md-2 col-lg-2 col-form-label"></label>
                <div class="col-sm-8 col-md-10 col-lg-10 input-group">
                    <h4>Your session is about to expire!</h4>
                </div>
            </div>           
            <div class="form-group">
                <label class="col-sm-4 col-md-4 col-lg-3 col-form-label"></label>
                <div class="col-sm-6 col-md-6 col-lg-8 input-group" style="margin: 20px 0px 20px 0px;">                   
                    <span class="btn-primary" id="sessionlink" style="border-radius: 25px;padding: 5px 20px;color:#fff;">
                        @*<a asp-area="" asp-controller="csat_login" asp-action="csat_login" class="btn-primary btn-sm Switcher pull-left ">Go back to login page</a>*@
                        @Html.ActionLink("Go back to login page", "csat_login", "csat_login",null, new { @class = "title" })
                    </span>
                </div>
            </div>
        </form>
    </div>
    <div class="col-lg-4 col-md-3 col-sm-3 col-xs-2"></div>
</div>

