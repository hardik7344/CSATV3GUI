
@{
    ViewData["Title"] = "Modem usage_report";
    Layout = "~/Views/Shared/ownyit_main_page.cshtml";
}
<body onload="fn_loading()">
    <div class="row">
        <!--side menu-->

        <div class="ibox float-e-margins ">
            <div class="ibox-content" style="padding:4px;">
                <ul class="breadcrumb" style="margin-bottom:0px;">
                    <li>
                        @Html.ActionLink("Reports", "report_agent_information", "csat_report")
                        <i class="icon-angle-right"></i>
                    </li>
                    <li> Internet Modem Usage  </li>
                </ul>
            </div>
        </div>
        <div class="col-sm-2 col-md-2 col-lg-2 no_padding">
            <div class="ibox float-e-margins ">
                <div class="ibox-content sidemenu">
                    @{
                        @Html.Partial("csat_report_menu");
                    }
                </div>
            </div>
        </div>
        <!--side menu end-->
        <div class="col-sm-10 col-md-10 col-lg-10">
            <div class="ibox float-e-margins" style="margin-bottom: 10px;">
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="row">
                                <div class="col-xs-12">
                                    <form class="form-horizontal" method="post">
                                        <div class="form-group">
                                            <label for="nameField" style="margin-top: -5px;background-color: #fff;text-align: center;color: #3D93C8;width: 120px;" class="col-xs-2 form_label">&nbsp;&nbsp;Advance Search&nbsp;&nbsp;</label>
                                            <div class="col-xs-10">
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="row">
                                <div class="col-xs-12">
                                    <form class="form-horizontal" method="post" name="frm_modemusage_report" id="frm_modemusage_report">
                                        <div class="form-group">
                                            <label for="nameField" class="col-xs-1 form_label">Branch/ Unit</label>
                                            <div class="col-xs-4">
                                                <select class="form-control col-sm-3 col-lg-3 col-md-3" name="Branch/Unit" id="DrpOU"></select>
                                            </div>
                                            <label for="nameField" class="col-xs-1 form_label">System Name</label>
                                            <div class="col-xs-2" id="selectsys">
                                                <select class="form-control col-sm-3 col-lg-3 col-md-3" name="Device" id="system">
                                                    <option value="-1">Select System Name</option>";
                                                </select>
                                            </div>
                                            <div class="col-xs-2" id="inputsystemname">
                                                <input type="text" class="form-control col-sm-3 col-lg-3 col-md-3" name="add_systemname" id="add_systemname">
                                            </div>
                                            <div class="col-xs-1">
                                                <input type="checkbox" class="form-check-input" id="add_system_chk" name="add_system_chk" style="width:20px;height:25px;">
                                            </div>
                                            <label for="nameField" class="col-xs-1 form_label" style="margin-left:-48px;">IP Address</label>
                                            <div class="col-xs-2">
                                                <input type="text" class="form-control col-sm-3 col-lg-3 col-md-3" placeholder="" name="ip" id="ip">
                                                @*<select class="form-control col-sm-3 col-lg-3 col-md-3" name="ip" id="ip">
                <option value="-1">Select IP Address</option>
            </select>*@
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="nameField" class="col-xs-1 form_label">Search</label>
                                            <div class="col-xs-2">
                                                <select class="form-control col-sm-2 col-lg-2 col-md-2" name="type" id="serchtype">
                                                    <option>Select Type</option>
                                                    <option value="T.login_user">Username</option>
                                                    <option value="T.device_type">Modem Name</option>
                                                    <option value="starttime">Start time</option>
                                                    <option value="endtime">End time</option>
                                                    <option value="Duration">Duration</option>
                                                </select>
                                            </div>
                                            <div class="col-xs-3">
                                                <div class="input-group">
                                                    <input type="search" class="form-control" placeholder="Search" id="serchvalue" name="serchvalue" autocomplete="off">
                                                    <div class="input-group-btn">
                                                        <button class="btn btn-default" type="submit">
                                                            <i class="glyphicon glyphicon-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="pull-left col-xs-1">
                                                <input type="button" class="btn btn-primary" value="Show" id="btnshowreport">
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--form  content end-->
            <!--MODEM report form end-->
            <!--MODEM report Datatable start-->
            <div class="row ibox2">
                <div class="col-sm-12 col-md-12 col-lg-12 ibox2-content" id="divmodem_div">
                    <table id="divmodem" class="table table-striped table-bordered " width="100%"></table>
                </div>
            </div>
            <div class="space_bottom">&nbsp;</div>
        </div>
    </div>
</body>
<script>    
    $(document).ready(function () {
        validateSingleForm("#frm_modemusage_report");
    });
</script>
<script>
    function fn_loading() {
        $("#overlay").show();
        $("#overlay").fadeIn(300);
    }

    $.get('@Url.Content("~/csat_report/GetOU/")', null, DataBind)
    function DataBind(data) {      
	//debugger;
        var url = '@Url.Content("~/csat_report/GetOU/")';
        $.ajax({
            type: "POST",
            url: url,
            success: function (data) {
	            var SetData = $('#DrpOU');
			    jQuery('#DrpOU').html('');
			    var oudata = "<option >Select Branch/ Unit</option>";
			    for (var i = 0; i < data.ou.length; i++) {
			        oudata+="<option value='"+ data.ou[i].ouid +"'>"+ data.ou[i].ouname +"</option>";
                }
                SetData.append(oudata);
            }
        }).done(function () {
            setTimeout(function () {
                $("#overlay").fadeOut(300);
            }, 500);
        });
    }

    var ouidvalue="";
    var systemvalue="";
    var ipvalue="";
    var serchtype="";
    var ouid="";
    $("#DrpOU").change(function () {
        //  debugger;
        ouidvalue = this.value;
        var url = '@Url.Content("~/csat_report/Get_syatemname_ip?ouid=")' + this.value;
        $.ajax({
            type: "POST",
            url: url,
            success: function (data) {
		       	//debugger;
		    	 var SetDatadevice = $('#system');
				 //var SetDataip = $('#ip');
			     jQuery('#system').html('');
			     //jQuery('#ip').html('');
		        var devicedata = "<option value='-1'>Select System Name</option>";
		        //var ipdata = "<option value='-1'>Select IP Address</option>";
      	        for (var i = 0; i < data.systemip.length; i++) {
			        devicedata+="<option value='"+ data.systemip[i].device_name +"'>"+ data.systemip[i].device_name +"</option>";
				    //ipdata+="<option value='"+ data.systemip[i].ip +"'>"+ data.systemip[i].ip +"</option>";
				}
                SetDatadevice.append(devicedata);
				//SetDataip.append(ipdata);
            }

        });
    });

	$("#DrpOU").change(function () {
			ouid= this.value;
    });

	$("#system").change(function () {
			systemvalue= this.value;
    });

	//$("#ip").change(function () {
	//		ipvalue= this.value;
 //   });

	$("#serchtype").change(function () {
	    serchtype= this.value;
    });

	$("#btnshowreport").on('click',function()
    {
        var $valid = $('#frm_modemusage_report').valid();
        if (!$valid) {
            $validatorform.focusInvalid();
            return false;
        } else {
            $("#overlay").show();
            $("#overlay").fadeIn(300);
            // debugger;
            var ipvalue = document.getElementById("ip").value;
            var value = document.getElementById("serchvalue").value;
            var systemvalue = document.getElementById('add_systemname').value;
            if (systemvalue == "") {
                systemvalue = document.getElementById("system").value;
            }
            if (systemvalue == "") {
                systemvalue = "-1";
            }
            
            if (serchtype == "") {
                serchtype = "-1";
            }

            if (value == "") {
                value = "-1";
            }

            var url = '@Url.Content("~/csat_report/Get_modem_Report?systemname=")' + systemvalue + '&ip=' + ipvalue + '&fieldserach=' + serchtype + '&serachvalue=' + value + '&ouid=' + ouid;
            $.ajax({
                type: 'post',
                url: url,
                success: function (data) {
                    var dataclear = document.getElementById('divmodem_div');
                    dataclear.innerHTML = '&nbsp;';
                    $('#divmodem_div').append("<table id='divmodem' class='table table-striped table-bordered' width='100%'></table>");
                    otable = document.getElementById('divmodem');
                    var data1 = [];
                    for (var i = 0; i < data.modemusagedata.length; i++) {
                        data1.push([data.modemusagedata[i].ou_longname, data.modemusagedata[i].device_name, data.modemusagedata[i].ip, data.modemusagedata[i].login_user, data.modemusagedata[i].device_type, data.modemusagedata[i].starttime, data.modemusagedata[i].endtime, data.modemusagedata[i].duration]);
                    }
                    var otable = $("#divmodem").DataTable({
                        data: data1, scrollX: !0, searching: !1, dom: "<'top'lB>rt<'bottom'ip><'clear'>", buttons: ["colvis", { text: "PDF", extend: "pdfHtml5", download: "open", filename: "Internet Modem Usage_" + today, orientation: "portrait", pageSize: "A4", exportOptions: { columns: ":visible", search: "applied", order: "applied" }, customize: function (t) { t.content.splice(0, 1); var e = new Date, n = e.getDate() + "-" + (e.getMonth() + 1) + "-" + e.getFullYear(); t.pageMargins = [20, 60, 20, 30], t.defaultStyle.fontSize = 7, t.styles.tableHeader.fontSize = 7, t.header = function () { return { columns: [{ margin: [0, -5, 30, 15], alignment: "left", width: 100, image: "data:image/png;base64," + pdflogo }, { margin: [-10, 10, 10, 0], alignment: "center", width: 410, text: "Modem Usage Report", fontSize: 12 }, { alignment: "right", fontSize: 14, text: "" }], margin: 20 } }, t.footer = function (t, e) { return { columns: [{ alignment: "left", text: ["Created on: ", { text: n.toString() }] }, { alignment: "right", text: ["page ", { text: t.toString() }, " of ", { text: e.toString() }] }], margin: 20 } }; var a = { hLineWidth: function (t) { return .5 }, vLineWidth: function (t) { return .5 }, hLineColor: function (t) { return "#aaa" }, vLineColor: function (t) { return "#aaa" }, paddingLeft: function (t) { return 4 }, paddingRight: function (t) { return 4 } }; t.content[0].layout = a } }, 'excel'],
                        columns: [{ title: "Branch/ Unit" },{ title: "System Name" }, { title: "IP Address" }, { title: "Username" }, { title: "Modem Name" }, { title: "Start Time" }, { title: "End Time" }, { title: "Duration" }]
                    });
                    $(".table").css({ "width": "100%" });
                    $(".dataTables_scrollHeadInner ").css({ "width": "100%" });
                    document.getElementById("serchvalue").value = '';
                }
            }).done(function () {
                setTimeout(function () {
                    $("#overlay").fadeOut(300);
                }, 500);
            });
        }
	});
</script>
<script>
    //checkbox input hide & show
    $('#inputsystemname').hide();
    $('#add_system_chk').on('click', function () {
        if ($(this).is(':checked')) {
            $('#selectsys').hide();
            $('#inputsystemname').show();
        }
        else {
            $('#selectsys').show();
            $('#inputsystemname').hide();
            document.getElementById('add_systemname').value = "";
        }
    });
</script>