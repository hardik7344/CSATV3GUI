<script>
    $(document).ready(function () {
        validateSingleForm("#frm_mngagentregistry_report");
    });
</script>
    <script>
    var strdate="";
    var enddate="";
    $(function () {
        $('input[name="datereg"]').daterangepicker({
            autoUpdateInput: true,
            timePicker: true,

            timePickerSeconds: true,
            timePicker24Hour: true,
            startDate: moment().add(-24, 'hour'),
            endDate: moment(),
            locale: {
                cancelLabel: 'Clear',
                format: 'DD/MM/YYYY hh:mm:ss '
            }
        });

        $('input[name="datereg"]').on('apply.daterangepicker', function (ev, picker) {
            $(this).val(picker.startDate.format('DD/MM/YYYY hh:mm:ss') + ' - ' + picker.endDate.format('DD/MM/YYYY hh:mm:ss'));
            strdate = picker.startDate.format('DD/MM/YYYY hh:mm A');
            enddate = picker.endDate.format('DD/MM/YYYY hh:mm A');
        });

        $('input[name="datereg"]').on('cancel.daterangepicker', function (ev, picker) {
            $(this).val('');
        });
        $(window).bind('scroll', function () {
            $(".daterangepicker").hide();
        });

    });
    var ouidvalue = "";
$.get('@Url.Content("~/csat_report/GetOU/")', null, DataBind)
    function DataBind(data) {
        $("#overlay").show();
        $("#overlay").fadeIn(300);
       // debugger;
        var url = '@Url.Content("~/csat_report/GetOU/")';
        $.ajax({
            type: "POST",
            url: url,
            success: function (data) {
        var SetData = $('#drpou');
        jQuery('#drpou').html('');

        var oudata = "<option value='-1'>Select Branch/ Unit</option>";
        for (var i = 0; i < data.ou.length; i++) {
            oudata += "<option value='" + data.ou[i].ouid + "'>" + data.ou[i].ouname + "</option>";
        }
        SetData.append(oudata);
            }
        }).done(function () {
            setTimeout(function () {
                $("#overlay").fadeOut(300);
            }, 500);
        });
    }
    $("#drpou").change(function () {
        ouidvalue = this.value;
        var url = '@Url.Content("~/csat_report/Get_syatemname_ip?ouid=")' + this.value;
        $.ajax({
        type: "POST",
        url: url,
        success: function (data) {
        var SetDatadevice = $('#drpdevice');
        //var SetDataip = $('#drpip');

        jQuery('#drpdevice').html('');
        //jQuery('#drpip').html('');

        var devicedata = "<option value='-1'>Select System Name</option>";
        //var ipdata = "<option value='-1'>Select IP Address</option>";

        for (var i = 0; i < data.systemip.length; i++) {
           devicedata+="<option value='"+ data.systemip[i].device_name +"'>"+ data.systemip[i].device_name +"</option>";
           //ipdata+="<option value='"+ data.systemip[i].ip +"'>"+ data.systemip[i].ip +"</option>";
            }

            SetDatadevice.append(devicedata);
            //SetDataip.append(ipdata);

        }

        });

    });
    var typeSerach = "";
    var ValueSerach = "";
        function Funcagentreginfo() {
            var $valid = $('#frm_mngagentregistry_report').valid();
            if (!$valid) {
                $validatorform.focusInvalid();
                return false;
            } else {
                $("#overlay").show();
                $("#overlay").fadeIn(300);
                //debugger;
                startdate = $('#dateagentreg').data('daterangepicker').startDate.format('DD/MM/YYYY');
                enddate = $('#dateagentreg').data('daterangepicker').endDate.format('DD/MM/YYYY');

                ValueSerach = document.getElementById("searchvalue").value;
                typeSerach = document.getElementById("type").value;
                //devicevalue = document.getElementById("drpdevice").value;
                var devicevalue = document.getElementById('add_systemname').value;
                if (devicevalue == "") {
                    devicevalue = document.getElementById("drpdevice").value;
                }
                ipvalue = document.getElementById("drpip").value;
                var url = '@Url.Content("~/csat_report/GetManageRegData?ouid=")' + ouidvalue + '&ip=' + ipvalue + '&device=' + devicevalue + '&startdate=' + strdate + '&enddate=' + enddate + '&fieldserach=' + typeSerach + '&valuesearch=' + ValueSerach;

                $.ajax({
                    type: "POST",
                    url: url,
                    success: function (data) {
                        var dataclear = document.getElementById('divagentreg_div');
                        dataclear.innerHTML = '&nbsp;';
                        $('#divagentreg_div').append("<table id='divagentreg' class='table table-striped table-bordered' width='100%'></table>");
                        otable = document.getElementById('divagentreg');
                        var data1 = [];
                        for (var i = 0; i < data.agentregdata.length; i++) {
                            data1.push([data.agentregdata[i].ou_longname, data.agentregdata[i].device_name, data.agentregdata[i].ip, data.agentregdata[i].timestamp, data.agentregdata[i].c057path, data.agentregdata[i].c057key, data.agentregdata[i].c057keyvalue, data.agentregdata[i].keytype, data.agentregdata[i].action, data.agentregdata[i].status]);
                        }
                        var otable = $("#divagentreg").DataTable({
                            data: data1, scrollX: !0, searching: !1, dom: "<'top'lB>rt<'bottom'ip><'clear'>", buttons: ["colvis", { text: "PDF", extend: "pdfHtml5", download: "open", filename: "Manage Agent-Registry_" + today, orientation: "portrait", pageSize: "A4", exportOptions: { columns: ":visible", search: "applied", order: "applied" }, customize: function (t) { t.content.splice(0, 1); var e = new Date, n = e.getDate() + "-" + (e.getMonth() + 1) + "-" + e.getFullYear(); t.pageMargins = [20, 60, 20, 30], t.defaultStyle.fontSize = 7, t.styles.tableHeader.fontSize = 7, t.header = function () { return { columns: [{ margin: [0, -5, 30, 15], alignment: "left", width: 100, image: "data:image/png;base64," + pdflogo }, { margin: [-10, 10, 10, 0], alignment: "center", width: 410, text: "Manage Agent-Registry Report", fontSize: 12 }, { alignment: "right", fontSize: 14, text: "" }], margin: 20 } }, t.footer = function (t, e) { return { columns: [{ alignment: "left", text: ["Created on: ", { text: n.toString() }] }, { alignment: "right", text: ["page ", { text: t.toString() }, " of ", { text: e.toString() }] }], margin: 20 } }; var a = { hLineWidth: function (t) { return .5 }, vLineWidth: function (t) { return .5 }, hLineColor: function (t) { return "#aaa" }, vLineColor: function (t) { return "#aaa" }, paddingLeft: function (t) { return 4 }, paddingRight: function (t) { return 4 } }; t.content[0].layout = a } }, 'excel'],
                            columns: [{ title: "Branch/ Unit" }, { title: "System Name" }, { title: "IP Address" }, { title: "Timestamp" }, { title: "Path" }, { title: "Key" }, { title: "Key Value" }, { title: "Key Type" }, { title: "Action" }, { title: "Status" }]
                        });
                        $(".table").css({ "width": "100%" });
                        $(".dataTables_scrollHeadInner ").css({ "width": "100%" });
                    }
                }).done(function () {
                    setTimeout(function () {
                        $("#overlay").fadeOut(300);
                    }, 500);
                });
            }
    }
</script>

    <script>
        //checkbox input hide & show
        $('#inputsystemname').hide();
        $('#add_system_chk').on('click', function () {
            if ($(this).is(':checked')) {
                $('#selectsys').hide();
                $('#inputsystemname').show();
            }
            else {
                $('#selectsys').show();
                $('#inputsystemname').hide();
                document.getElementById('add_systemname').value = "";
            }
        });
    </script>